// See https://go.microsoft.com/fwlink/?LinkId=733558
// for the documentation about the tasks.json format
{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "npm: compile-web",
			"type": "shell",
			"command": "bash ./npm-run.sh compile",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": [
				"$ts-webpack",
				"$tslint-webpack"
			]
		},
		{
			"label": "npm: watch-web",
			"type": "shell",
			"command": "bash ./npm-run.sh watch",
			"group": "build",
			"isBackground": true,
			"problemMatcher": [
				"$ts-webpack-watch",
				"$tslint-webpack-watch"
			]
		}
	]
}
